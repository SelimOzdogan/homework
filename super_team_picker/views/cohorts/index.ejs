<%- include('../partials/header.ejs') %>
<div class="container">
    <h1>Cohorts</h1>

    <div class="post-card">
        <% for(let cohort of cohorts) { %>
        <div class="card mb-4">
            <div class="cardheader">
                <img class="card-img float-left" src="<%= cohort.logoUrl %>">
                <div class="cardname float-left"><%= cohort.name %> </div>
            </div>
            <p class="card-text"><%= cohort.members %> </p>
            <input class="btn btn-primary" type="submit" value="Show cohort" />
        </div>
        <% } %>
    </div>
</div>
<%- include('../partials/footer.ejs') %>