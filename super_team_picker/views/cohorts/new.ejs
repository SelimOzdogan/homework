<%- include("../partials/header.ejs") %>
<div class="container">

<form action="/cohorts" method="POST">
    <div class="form-group">
        <label for="logoUrl">Logo Url</label><br />
        <input class="form-control" type="url" name="logoUrl" placeholder="e.g. https://www.example.com/some.png" />
    </div>
    <div class="form-group">
        <label for="name">Name</label><br />
        <input class="form-control" type="text" name="name" />
    </div>
    <div class="form-group">
        <label for="members">Members</label><br />
        <textarea class="form-control"  name="members" class="form-control" placeholder="Jon Snow, Daenerys, Arya Stark"></textarea>
    </div>
    <input type="submit" class="btn btn-outline-success" value="Submit" />
</form>
</div>
<%- include("../partials/footer.ejs") %>